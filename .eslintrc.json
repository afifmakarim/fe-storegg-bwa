{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "airbnb/base",
        "plugin:react/recommended",
        "standard-with-typescript"
        
    ],
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "avoidEscape": true,
        "allowTemplateLiterals": true
      }
}
